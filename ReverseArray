import java.util.*;
public class Main {
	public static void main(String[] args) {
		int[] arr = {1, 2, 3, 4, 5};
		
		System.out.println(Arrays.toString(reverseArr(arr)));
	}
	public static int[] reverseArr(int[] arr){
	    int temp;
	    int i = 0;
	    int j = arr.length -1;
	    
	    while(i < j){
	        temp = arr[i];
	        arr[i] = arr[j];
	        arr[j] = temp;
	        i++;
	        j--;
	    }
	    
	   return arr; 
	}
}